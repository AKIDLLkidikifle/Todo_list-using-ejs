<%- include("header") %>
  <div class="box" id="heading">
    <h1><%= title %></h1>
  </div>

  <div class="box">
      <% for(var i=0; i< addedElement.length; i++){ %>
          <div class="item">
            <input type="checkbox">
            <p><%= addedElement[i] %></p>
          </div>   
      <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="todo" placeholder="New Item">
        <button type="submit" name="list" value=<%= title %>>+</button>
    </form>
  </div>
  <%- include("footer") %>
  

  

    

